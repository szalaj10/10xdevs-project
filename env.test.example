# Test Environment Variables
# Copy this file to .env.test and fill in your test credentials

# Supabase Configuration (for local testing)
# Get these values by running: supabase status
PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
PUBLIC_SUPABASE_KEY=your-anon-key-here

# Test User Credentials (for E2E tests)
# These should match the credentials of a test user in your Supabase database
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=test123456

# Base URL for E2E tests
BASE_URL=http://localhost:4321

# Optional: GROQ API Configuration (if testing AI features)
GROQ_API_KEY=your-groq-api-key-here
GROQ_MODEL=llama-3.3-70b-versatile
GROQ_BASE_URL=https://api.groq.com/openai/v1

